import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int pares = 0;
		int impares = 0;
		
		for (int i = 1 ; i < x ; i++) {
			System.out.println(i);
			
		}
		
		System.out.println(pares);
		System.out.println(impares);
		
		sc.close();
	}
}