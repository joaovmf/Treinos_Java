import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
	
		System.out.println("enter three numbers : ");
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		
		if (a > c && a > b) {
			System.out.println(" the largest number entered is : " + a);
		} else if (b > c && b > a) {
			System.out.println(" the largest number entered is : " + b);
		} else {
			System.out.println(" the largest number entered is : " + c);
		}

		sc.close();
	}
}