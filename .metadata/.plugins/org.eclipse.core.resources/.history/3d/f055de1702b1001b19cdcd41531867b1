import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int pares = 0;
		int impares = 0;
		
		for (int i = 1 ; i < x ; i++) {
			if (i % 2 == 0) {
				pares = pares + 1;
			} else {
				impares = impares + 1;
			}
			
		}
		
		System.out.println(pares);
		System.out.println(impares);
		
		sc.close();
	}
}